{
    "name": "10fastfingers auto typer",
    "version": "1.0",
    "permissions": [
        "storage",
        "tabs",
        "*://*/*"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Auto Typer Speed",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "24": "icons/icon24.png",
        "48": "icons/icon48.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png"
    },
    "description": "This extension will type for you on the 10fastfingers typing speed test, at speed of your selection.",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}