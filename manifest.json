{
    "name": "10fastfingers auto typer",
    "version": "1.0",
    "permissions": ["storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Auto Typer Speed",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/get_started16.png",
        "32": "images/get_started32.png",
        "48": "images/get_started48.png",
        "128": "images/get_started128.png"
    },
    "description": "This extension will type for you on the 10fastfingers typing speed test, at speed of your selection.",
    "manifest_version": 2
}